{
  "manifest_version": 3,
  "name": "Volt",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Volt"
  },
  "content_scripts": [
    {
      "matches": [
        "https://fasah.zatca.gov.sa/*",
        "https://oga.fasah.sa/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}