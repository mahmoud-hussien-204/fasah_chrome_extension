{
  "manifest_version": 3,
  "name": "Volt",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Click to open popup"
  },
  "content_scripts": [
    {
      "matches": [
        "https://oga.fasah.sa/*",
        "https://fasah.zatca.gov.sa/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}